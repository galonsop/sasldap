{
    "name": "galonsop/sasldap",
    "description": "LDAP Authentication & Management for SAS.",
    "keywords": [
        "ldaprecord",
        "adldap2",
        "ldap",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=8.0.2",
        "illuminate/support": "^9.0",
        "directorytree/ldaprecord-laravel": "^2.7",
        "ext-ldap": "*"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0|^8.0|^9.0",
        "orchestra/testbench": "^3.7|^4.0|^5.0|^6.0|^7.0"
    },
    "archive": {
        "exclude": [
            "/tests"
        ]
    },
    "autoload": {
        "psr-4": {
            "SASLdap\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SASLdap\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SASLdap\\SASLdapServiceProvider"
            ]
        }
    }
}
